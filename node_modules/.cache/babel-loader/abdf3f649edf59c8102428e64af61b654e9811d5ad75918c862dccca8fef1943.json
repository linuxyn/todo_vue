{"ast":null,"code":"// import axios from 'axios'\nexport default {\n  name: 'githubSearch',\n  data() {\n    return {\n      keyword: ''\n    };\n  },\n  methods: {\n    searchUsers() {\n      if (!this.keyword.trim()) return alert('输入不能为空');\n      // 请求前更新List数据,因为isFrist每次都要传，值都是一样的比较繁琐，这里可以省略，后面需要特殊处理\n      this.$bus.$emit('updateListData', {\n        isFrist: false,\n        isLoading: true,\n        errorMsg: '',\n        users: []\n      });\n      this.$http.get(`https://api.github.com/search/users?q=${this.keyword}`).then(response => {\n        console.log('请求成功了', response.data);\n        // 请求成功后更新List数据\n        this.$bus.$emit('updateListData', {\n          isLoading: false,\n          errorMsg: '',\n          users: response.data.items\n        });\n      }, error => {\n        console.log('请求失败了', error.message);\n        // 请求失败后更新List数据\n        this.$bus.$emit('updateListData', {\n          isLoading: false,\n          errorMsg: error.message,\n          users: []\n        });\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAYA;AACA;EACAA;EACAC;IACA;MACAC;IACA;EACA;EACAC;IACAC;MACA;MACA;MACA;QAAAC;QAAAC;QAAAC;QAAAC;MAAA;MACA,6EACAC;QACAC;QACA;QACA;UAAAJ;UAAAC;UAAAC;QAAA;MACA,GACAG;QACAD;QACA;QACA;UAAAJ;UAAAC;UAAAC;QAAA;MACA,EACA;IACA;EACA;AACA","names":["name","data","keyword","methods","searchUsers","isFrist","isLoading","errorMsg","users","response","console","error"],"sourceRoot":"src/components","sources":["Search.vue"],"sourcesContent":["<template>\n    <section class=\"jumbotron\">\n    <h3 class=\"jumbotron-heading\">Search Github Users</h3>\n    <div class=\"search-box\">\n      <input type=\"text\" placeholder=\"enter the name you search\" v-model=\"keyword\"/>&nbsp;\n      \n      <!-- <button @click=\"searchUsers\">Search</button> -->\n      <button @click=\"searchUsers\" class=\"Search\">Search</button>\n    </div>\n  </section>\n</template>\n<script>\n  // import axios from 'axios'\n  export default {\n    name:'githubSearch',\n    data() {\n      return {\n        keyword:''\n      }\n    },\n    methods: {\n      searchUsers(){\n        if(!this.keyword.trim()) return alert('输入不能为空')\n        // 请求前更新List数据,因为isFrist每次都要传，值都是一样的比较繁琐，这里可以省略，后面需要特殊处理\n        this.$bus.$emit('updateListData',{isFrist:false,isLoading:true,errorMsg:'',users:[]})\n        this.$http.get(`https://api.github.com/search/users?q=${this.keyword}`).then(\n          response => {\n            console.log('请求成功了', response.data)\n            // 请求成功后更新List数据\n            this.$bus.$emit('updateListData',{isLoading:false,errorMsg:'',users:response.data.items})\n          },\n          error => {\n            console.log('请求失败了', error.message)\n            // 请求失败后更新List数据\n            this.$bus.$emit('updateListData',{isLoading:false,errorMsg:error.message,users:[]})\n          }\n        )\n      }\n    },\n  }\n</script>\n\n<style>\n.search-box {\n  display: flex;\n  align-items: center;\n  width: 300px;\n  height: 40px;\n  border: 2px solid #ccc;\n  border-radius: 2px;\n  overflow: hidden;\n}\n\n.search-box input[type=\"text\"] {\n  flex: 1;\n  height: 100%;\n  font-size: 16px;\n  border: none;\n  outline: none;\n  padding-left: 10px;\n}\n\n.Search{\n  border-style:none;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}